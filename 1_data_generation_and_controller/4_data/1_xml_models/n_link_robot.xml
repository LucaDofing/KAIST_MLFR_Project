<?xml version="1.0" ?>
<mujoco model="n_link_robot">
	<compiler angle="radian" inertiafromgeom="true"/>
	<asset>
		<texture type="skybox" builtin="gradient" rgb1="0.6 0.6 0.6" rgb2="0.3 0.3 0.3" width="512" height="512"/>
		<texture name="texplane" type="2d" builtin="checker" rgb1="0.0 0.0 0.0" rgb2="1.0 1.0 1.0" width="512" height="512"/>
		<material name="groundmat" texture="texplane" texrepeat="5 5"/>
	</asset>
	<visual>
		<map znear="0.001"/>
		<scale forcewidth="0.1"/>
	</visual>
	<default>
		<joint armature="1" damping="1" limited="true"/>
		<geom contype="0" friction="1 0.1 0.1" rgba="1 1 1 1"/>
	</default>
	<option gravity="0 -9.81 0" integrator="RK4" timestep="0.01"/>
	<worldbody>
		<camera name="track" mode="trackcom" pos="0 -0.3 0.3" xyaxes="1 0 0 0 1 1" target="body0"/>
		<geom conaffinity="0" contype="0" name="ground" pos="0 0 0" material="groundmat" size="1 1 10" type="plane"/>
		<light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3" specular=".1 .1 .1"/>
		<geom conaffinity="0" contype="0" fromto="0 0 0 0 0 0.02" name="root" rgba="0.8 0.3 0.5 1" size=".011" type="cylinder"/>
		<body name="body0" pos="0 0 .01">
			<joint axis="0 0 1" name="joint0" pos="0 0 0" type="hinge" limited="false"/>
			<geom fromto="0 0 0 0.15 0 0" name="link0" rgba="0.0 0.3 0.5 1" size="0.01" type="capsule" density="1000.0"/>
			<body name="body1" pos="0.15 0 0">
				<joint axis="0 0 1" name="joint1" pos="0 0 0" type="hinge" limited="true" range="-2.0 2.0"/>
				<geom fromto="0 0 0 0.15 0 0" name="link1" rgba="0.0 0.3 0.5 1" size="0.01" type="capsule" density="1000.0"/>
				<body name="fingertip" pos="0.16 0 0">
					<geom contype="0" name="fingertip" pos="0 0 0" rgba="0.0 0.7 0.5 1" size="0.01" type="sphere" density="1000.0"/>
				</body>
			</body>
		</body>
		<geom conaffinity="0" contype="0" name="target" pos="0.15 0.15 .01" rgba="0.8 0.1 0.1 1" size=".009" type="sphere"/>
	</worldbody>
	<actuator>
		<motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="150.0" joint="joint0"/>
		<motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="150.0" joint="joint1"/>
	</actuator>
</mujoco>
